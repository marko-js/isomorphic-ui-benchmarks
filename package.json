{
  "name": "isomorphic-ui-benchmarks",
  "version": "1.0.0",
  "description": "",
  "main": "./server.js",
  "scripts": {
    "test": "npm run benchmark",
    "start": "NODE_ENV=production node server.js",
    "benchmark": "NODE_ENV=production node --expose-gc benchmark-server/run.js",
    "build": "npm run bundle --silent && npm run minify --silent",
    "bundle": "node ./scripts/bundle.js",
    "static": "node ./scripts/static.js",
    "publish": "node ./scripts/publish.js",
    "minify": "node ./scripts/minify.js",
    "build-inferno": "npm run bundle-inferno --silent && node ./scripts/minify.js inferno",
    "build-infernoOptimized": "npm run bundle-infernoOptimized --silent && node ./scripts/minify.js infernoOptimized",
    "build-marko": "npm run bundle-marko --silent && node ./scripts/minify.js marko",
    "build-vue": "npm run bundle-vue --silent && node ./scripts/minify.js vue",
    "bundle-inferno": "node ./scripts/rollup.js inferno",
    "bundle-infernoOptimized": "node ./scripts/rollup.js infernoOptimized",
    "bundle-marko": "node ./scripts/rollup.js marko",
    "bundle-react": "node ./scripts/rollup.js react",
    "bundle-preact": "node ./scripts/rollup.js preact",
    "bundle-vue": "npm run bundle-vue-server && node ./scripts/rollup.js vue",
    "bundle-vue-server": "webpack --config benchmarks/color-picker/vue/webpack.config.js && webpack --config benchmarks/search-results/vue/webpack.config.js"
  },
  "author": "Patrick Steele-Idem <pnidem@gmail.com",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "babel-plugin-inferno": "4.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "benchmark": "^2.1.4",
    "browser-refresh-taglib": "^1.1.0",
    "compression": "^1.7.2",
    "envify": "^4.1.0",
    "express": "^4.16.2",
    "format-number": "^3.0.0",
    "google-closure-compiler-js": "^20180204.0.0",
    "inferno": "4.0.6",
    "inferno-server": "4.0.6",
    "inferno-vnode-flags": "4.0.6",
    "lasso": "2.11.24",
    "lasso-marko": "^2.4.0",
    "lodash": "^4.17.5",
    "marko": "^4.8.0",
    "markoify": "^2.2.2",
    "minprops": "^1.0.0",
    "mkdirp": "^0.5.1",
    "preact": "^8.2.7",
    "preact-render-to-string": "^3.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "require-self-ref": "^2.0.1",
    "rollup": "^0.56.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-marko": "0.0.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "safe-access": "^0.1.0",
    "serve-static": "^1.13.2",
    "uglify": "^0.1.5",
    "vue": "^2.5.13",
    "vue-loader": "^14.1.1",
    "vue-server-renderer": "^2.5.13",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "rollup-plugin-browserify-transform": "^1.0.1",
    "webpack-cli": "^2.0.9"
  }
}
