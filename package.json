{
  "name": "isomorphic-ui-benchmarks",
  "version": "1.0.0",
  "description": "",
  "main": "./server.js",
  "scripts": {
    "test": "npm run benchmark",
    "start": "node server.js",
    "benchmark": "npm run bundle-vue-server && node --expose-gc benchmark-server/run.js",
    "build": "npm run bundle --silent && npm run minify --silent",
    "bundle": "node ./scripts/bundle.js",
    "static": "node ./scripts/static.js",
    "publish": "node ./scripts/publish.js",
    "minify": "node ./scripts/minify.js",
    "build-inferno": "npm run bundle-inferno --silent && node ./scripts/minify.js inferno",
    "build-marko": "npm run bundle-marko --silent && node ./scripts/minify.js marko",
    "build-vue": "npm run bundle-vue --silent && node ./scripts/minify.js vue",
    "bundle-inferno": "node ./scripts/rollup.js inferno",
    "bundle-marko": "node ./scripts/rollup.js marko",
    "bundle-react": "node ./scripts/rollup.js react",
    "bundle-preact": "node ./scripts/rollup.js preact",
    "bundle-vue": "npm run bundle-vue-server && node ./scripts/rollup.js vue",
    "bundle-vue-server": "webpack --config benchmarks/color-picker/vue/webpack.config.js && webpack --config benchmarks/search-results/vue/webpack.config.js",
    "format": "prettier \"**/*.{json,md,js,jsx}\" --write && eslint ."
  },
  "author": "Patrick Steele-Idem <pnidem@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@lasso/marko-taglib": "^1.0.15",
    "@marko/rollup": "^2.0.1",
    "@vue/babel-preset-jsx": "^1.1.1",
    "babel-plugin-inferno": "^6.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "benchmark": "^2.1.4",
    "browser-refresh-taglib": "^1.1.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "format-number": "^3.0.0",
    "inferno": "^7.3.2",
    "inferno-hydrate": "^7.3.2",
    "inferno-server": "^7.3.2",
    "lasso": "^3.3.1",
    "lasso-marko": "^2.4.8",
    "lodash": "^4.17.21",
    "marko": "^4.18.16",
    "mkdirp": "^0.5.1",
    "preact": "^10.0.1",
    "preact-render-to-string": "^5.0.7",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rollup": "^1.25.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "serve-static": "^1.14.1",
    "terser": "^4.3.9",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  }
}
